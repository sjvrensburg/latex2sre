{
  "name": "latex2sre",
  "version": "1.0.0",
  "description": "Convert LaTeX math to Speech Rule Engine JSON",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:cjs": "esbuild index.js --bundle --platform=node --format=cjs --outfile=bundled-app.cjs --minify",
    "build:sea:blob": "node --experimental-sea-config sea-config.json",
    "build:sea": "npm run build:cjs && npm run build:sea:blob && node build-sea-platform.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjvrensburg/latex2sre.git"
  },
  "keywords": [
    "latex",
    "mathjax",
    "speech-rule-engine",
    "SEA",
    "binary"
  ],
  "author": "Stefan Janse van Rensburg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjvrensburg/latex2sre/issues"
  },
  "homepage": "https://github.com/sjvrensburg/latex2sre#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "esm": "^3.2.25",
    "mathjax-full": "^3.2.2",
    "speech-rule-engine": "4.1.2"
  },
  "devDependencies": {
    "esbuild": "^0.19.11",
    "postject": "^1.0.0"
  }
}
